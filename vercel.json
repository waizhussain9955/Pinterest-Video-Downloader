{
  "version": 2,
  "builds": [
    {
      "src": "api/pinterest.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/pinterest",
      "destination": "/api/pinterest.js"
    }
  ],
  "functions": {
    "api/pinterest.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
}